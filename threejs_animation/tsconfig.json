{
    "compilerOptions": {
        "target": "ES2017",
        "lib": ["dom", "dom.iterable", "esnext"],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "noEmit": true,
        "incremental": true,
        "module": "esnext",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "plugins": [
            {
                "name": "next"
            }
        ]
    },
    "include": [
        "next-env.d.ts",
        ".next/types/**/*.ts",
        "**/*.ts",
        "**/*.tsx",
        "src/app/components/ThreeScene.jsx",
        "src/app/components/StarfieldBackground.jsx",
        "src/components/oldcode/CubeScroll.jsx"
, "src/app/cubescroll/page.jsx", "src/app/animation/page.jsx"    ],
    "exclude": ["node_modules"]
}
